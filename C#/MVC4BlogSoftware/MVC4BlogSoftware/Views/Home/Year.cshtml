@model MVC4BlogSoftware.PagedEntries

<!-- import the included stylesheet for some (very basic) default styling -->
<link href="/Content/PagedList.css" rel="stylesheet" type="text/css" />
@Html.Partial("_ArchivePartial")
@if(Model.PagingEnabled)
{
    if (Model.CurrentPage > 1)
    {
        @Html.ActionLink("<<", "Year", new { year = Model.Year, page = Model.CurrentPage - 1});
    }
    else
    {
        <span>&lt;&lt;</span>
    }
    if (Model.PageOfPosts.Any())
    {
        @Html.ActionLink(">>", "Year", new { year = Model.Year, page = Model.CurrentPage + 1 });
    }
    else
    {
        <span>&gt;&gt;</span>
    }
    @Html.ActionLink("All one page?", "Year", new {year = Model.Year, page = 0})    
    
}
else
{
    @Html.ActionLink("5 per page?", "Year", new {year = Model.Year, page = 1})       
}