@model FirstSteps.Post
@{
        ViewBag.Title = "Home Page";
}
@if(Model != null)
{
    <ol class="round">
        <li>
            <h2>@Model.Title</h2>
            <h5>@Html.ActionLink(Model.Author, "Author", "Home", new {name = Model.Author})</h5>
            @Model.Body<br/>
            @Model.Published_DateTime
        </li>

    </ol>
    
}

